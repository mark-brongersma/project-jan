{% extends 'partials/_master.twig' %}

{% block main %}
    <div class="contact-container">
        <div class="contact-info">
            <h1>{{ record.title }}</h1>
            <p><strong>{{ record.name }}</strong></p>
            <p><strong>Email:</strong> <a href = "mailto: {{ record.email }}">{{ record.email }}</a></p>
        </div>
        <figure class="home-image">
            {% include 'partials/_image.twig' with ({'image': record.photo}) %}
        </figure>
    </div>
{% endblock main %}
